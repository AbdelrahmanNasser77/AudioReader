<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.audioreader_trialgui_fx2.HelloController">
  <children>
    <MenuBar fx:id="menuBar" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="572.0">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="openItem" mnemonicParsing="false" onAction="#handleOpen" text="Open" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Audio">
          <items>
            <MenuItem fx:id="playItem" disable="true" mnemonicParsing="false" onAction="#handlePlay" text="Play" />
            <MenuItem fx:id="displayHeaderItem" disable="true" mnemonicParsing="false" onAction="#handleDisplayHeader" text="Display Header" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
  </children>
</AnchorPane>
